
@{
    ViewBag.Title = "List";
    var newsList = ViewBag.data as string[];
}

<h2>List</h2>

<h3>第 @ViewBag.Page 页</h3>

<ol>
    @foreach1r item in newsList)
 { 
        <li>@item </li>
}

</ol>
<ol>
    @for (int i = 0; i < newsList.Length; i++)
    {
   <li>@newsList[i] </li>
    }

</ol>


<ol>
    @for (int i = 0; i < newsList.Length; i++)
    {
        <li>
@*<a herf="/Home/List?page=@i">第 @i页</a?*@
@Html.ActionLink(string.Format("第{0}页,i"), "List", "Home", new { page = i }, null )
</li>
}


</ol>